{
  "name": "@treasuredata/mcp-server",
  "version": "0.1.0",
  "description": "MCP server for Treasure Data - Query and interact with TD through Model Context Protocol",
  "main": "dist/index.js",
  "bin": {
    "td-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "treasure-data",
    "trino",
    "presto",
    "sql",
    "ai"
  ],
  "author": "Treasure Data",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/treasure-data/td-mcp-server.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "trino-client": "^0.2.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
